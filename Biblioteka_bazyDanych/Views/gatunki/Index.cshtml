@model IEnumerable<Biblioteka_bazyDanych.gatunki>

@{
    ViewBag.Title = "Index";
}

<h2>Gatunki</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>@Html.DisplayNameFor(model => model.nazwa)</th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.nazwa)
    </td>
    <td>
        @Html.ActionLink("Edit", "Edit", new { id = item.nazwa }) |
        @Html.ActionLink("Details", "Details", new { id = item.nazwa }) |
        @Html.ActionLink("Delete", "Delete", new { id = item.nazwa })
    </td>
</tr>
}

</table>
